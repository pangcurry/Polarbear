{
  "manifest_version": 2,

  "name": "Polarbear",
  "description": "This extension is Poloarbear",
  "version": "1.0.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "contextMenus",
    "activeTab",
    "downloads"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
